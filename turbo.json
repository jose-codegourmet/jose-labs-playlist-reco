{
  "globalEnv": [
    "ENV_SONGS_LIMIT",
    "ENV_OPENAI_API_KEY",
    "AI_API_URL",
    "NODE_ENV"
  ],
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["envAiApiUrl"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
